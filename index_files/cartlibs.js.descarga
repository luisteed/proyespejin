  var apedes = $("#txtapedes").val();
  var dirdes = $("#txtdirades").val();
  var ciudes = $("#txtciudes").val();
  var estdes = $("#txtestades").val();
  var copdes = $("#txtcoposdes").val();
  var tippag = $("#txtoptpay").val();

  var comped = $("#txtcomped").val();
  var refban = $("#txtrefpag").val();
  var imgban = $("#imgcompag").val();
  var refbanb = $("#txtrefpagb").val();
  var imgbanb = $("#imgcompagb").val();
  //PayMethod
  var opcunico = $(".payment_method input[name='txtformapago']:radio");
  var numcheck =0;
       $(".payment_method input[type='checkbox']").each(function(){
          if($(this).is(":checked")){
            numcheck = numcheck + 1;
          }          
        });
         
    $("#message-multipay").html(""); 

    if(!vtext.test(nomrem)){
     error=true;   
    } 
    if(!vtext.test(aperem)){
     error=true;   
    }
    if(!vtextnum.test(dirrem)){
     error=true;   
    }
    if(!vtext.test(ciurem)){
     error=true;   
    }
    if(!vtext.test(estrem)){
     error=true;   
    }
    if(!vnum.test(coprem)){
     error=true;   
    }
    if(!vnum.test(telrem)){
     error=true;   
    }
    if(!vemail.test(emarem)){
     error=true;   
    }
    if(tipago==0){
      error=true;
    }
    if(nwacco.is(":checked")){
      if(!vtextnum.test(pasrem)){
        error=true;
      }   
    }
    if(otrdes.is(":checked")){
        if(!vtext.test(nomdes)){
          error=true;
        }
        if(!vtext.test(apedes)){
          error=true;
        } 
        if(!vtextnum.test(dirdes)){
          error=true;
        } 
        if(!vtext.test(ciudes)){
          error=true;
        } 
        if(!vtext.test(estdes)){
          error=true;
        } 
        if(!vnum.test(copdes)){
          error=true;
        }   
    } 

    if(!vtextnum.test(comped)){
     error=true;   
    }    
    /*if(tippag=="directbank"){
      error=true;
    }*/
    if(tipago==1){
      if(!$(".payment_method input[name='txtformapago']:radio").is(":checked")){
       error=true;
      }
    }
    if(tipago==2){
      if(numcheck==0 || numcheck==1 || numcheck > 2){
       error=true;
       $("#message-multipay").html("<br><span class='alert alert-danger' rol='alert'>Solo puede seleccionar Dos Forma de Pago.</span><br>");
      }
    }
    if(tipago==1 || tipago==2){
      if(!vnum.test(refban)){
        error=true;
      }
      if(imgban==""){
        error=true;
      }
    }
    if(tipago==2){
      if(!vnum.test(refbanb)){
        error=true;
      }
      if(imgbanb==""){
        error=true;
      }
    }

    if(error==false){
    return false; 
    }else{
    return true;    
    }


}
